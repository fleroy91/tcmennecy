<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(res_sportif) -%>">
  <span class='title'>
    <%= res_sportif.titre %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.res_sportifs_res_sportif_path(res_sportif),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_res_sportifs_admin_res_sportif_path(res_sportif),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.res_sportifs_admin_res_sportif_path(res_sportif),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => res_sportif.titre),
        :method => :delete %>
  </span>
</li>
