<!--==============================content================================-->
  <section id="content">
    <div class="container_12">
      <div class="grid_12">
        <div class="box-shadow">
          <div class="wrap block-2">
            <div class="col-3">
                <%= presenter= SubMenuPresenter.new(sub_menu_pages(@page), self)
                  presenter.css = "side_menu"
                  presenter.dom_id = "side_menu"
                  presenter.to_html %>
                <%=raw @page.content_for(:side_body) %>
              </div>
            <div class="col-4">
              <%=raw @page.content_for(:body) %>
            </div>
          </div>
        </div>
      </div>
      <div class="clear"></div>
    </div>
  </section>
<% content_for :javascripts do %>
  <script>Cufon.now();</script>
<% end %>